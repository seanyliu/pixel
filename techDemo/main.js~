// Define globals
var GB_gameManager = null;
window.onload = init;

function init()
{
  var myCanvasHandle = document.getElementById("pixel_canvas");
  GB_gameManager = new GameManager(myCanvasHandle);
  GB_gameManager.start();
}

function button_start()
{
  GB_gameManager.start();
}

function button_stop()
{
  GB_gameManager.stop();
}


